<!DOCTYPE html>
<html>
<head>
  <title>HackDraft</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :all %>

	<%= yield(:head) %>

  <%= csrf_meta_tag %>
</head>
<body>
  <div id='container'>
    <%= yield %>
  </div>
<li class="navigation-item">
         	  <% if user_signed_in? %>
         	    Signed in as <%= current_user.email || current_user.user_name %>. Not you?
         	    <%= link_to "Sign out", destroy_user_session_path %>
         	  <% else %>
         	    <%= link_to "Sign in", new_user_session_path %> or
         	    <%#= link_to "Sign in with Github", user_omniauth_authorize_path(:github) %>
              or
              <%#= link_to "Sign in as User with meetup", user_omniauth_authorize_path(:meetup) %> 
              <%= link_to "sign up", new_user_session_path %>
         	  <% end %>
          
      </li>
  

</body>
</html>


