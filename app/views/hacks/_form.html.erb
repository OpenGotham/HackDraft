<%= form_for(@hack, :html =>
{:multipart => true}) do |f| %>
<div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div class="field">
     <%= f.label 'upload stack' %>

    <%= f.file_field :hack_stack  %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
  

<% end %>

